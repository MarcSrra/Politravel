<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.politravel">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/politravel"
        android:label="@string/app_name"
        android:roundIcon="@drawable/politravel"
        android:supportsRtl="true"
        android:theme="@style/Theme.Politravel"
        tools:targetApi="31">
        <activity
            android:name=".acttivities.ViatgeEdit"
            android:exported="false"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".acttivities.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.MyApp.Mysplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".acttivities.ViatgeDetail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".acttivities.FotoActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>